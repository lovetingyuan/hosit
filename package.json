{
  "name": "hosit",
  "version": "2.2.1",
  "description": "update the latest and available google hosts file",
  "main": "index.js",
  "scripts": {
    "test": "echo \"no test\"",
    "prebuild": "npm run lint",
    "build": "babel src -d lib",
    "lint": "eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/hosit.git"
  },
  "keywords": [
    "google",
    "hosts",
    "fq"
  ],
  "author": "tingyuan <1932294867@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lovetingyuan/hosit/issues"
  },
  "bin": {
    "hosit": "./bin/hosit"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/lovetingyuan/hosit#readme",
  "dependencies": {
    "async": "^2.5.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "debug": "^3.0.1",
    "is-ip": "^2.0.0",
    "lodash": "^4.17.4",
    "opn": "^5.1.0",
    "request": "^2.81.0"
  },
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "plugins": [
      "transform-constant-string"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "CONST": false
    },
    "rules": {
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "consistent-return": "off",
      "comma-dangle": "off"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-constant-string": "^0.1.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0"
  }
}
